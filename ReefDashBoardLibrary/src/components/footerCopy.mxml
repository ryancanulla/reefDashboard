<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009">
	<fx:Metadata>[HostComponent("spark.components.TextInput")]</fx:Metadata>
	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	<s:RichEditableText x="0" y="0" color="#666666" fontFamily="Calibri" fontSize="10" tabStops="S0 S50 S100" text="(Text)" width="67" heightInLines="1" id="textDisplay" y.normal="0" height.normal="11" width.normal="235" height.disabled="12" width.disabled="191" y.disabled="1"/>
	<s:transitions>
		<s:Transition fromState="normal" toState="disabled" autoReverse="true">
			<s:Parallel>
				<s:Parallel target="{textDisplay}">
					<s:Resize duration="0"/>
					<s:Move autoCenterTransform="true" duration="0"/>
				</s:Parallel>
			</s:Parallel>
		</s:Transition>
	</s:transitions>
</s:Skin>
